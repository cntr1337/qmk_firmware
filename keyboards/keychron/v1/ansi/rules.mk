# --- USTAWIENIA FABRYCZNE (TO NAPRAWI KLAWIATURE) ---
MCU = STM32L432
BOOTLOADER = stm32-dfu

# Wlaczenie przelacznika trybow (NAPRAWIA BLAD "QWERT")
DIP_SWITCH_ENABLE = yes

# Wlaczenie obslugi VIA
VIA_ENABLE = yes

# --- TWOJE DODATKI ---
# Wlaczenie myszki
MOUSEKEY_ENABLE = yes
EXTRAKEY_ENABLE = yes

# Optymalizacje
LTO_ENABLE = yes
NKRO_ENABLE = yes
